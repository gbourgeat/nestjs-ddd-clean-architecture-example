{
  "name": "nestjs-ddd-clean-architecture-example",
  "version": "0.0.1",
  "description": "A NestJS API for planning optimal routes between French cities using Clean Architecture, DDD patterns, and the Dijkstra algorithm with real-time weather conditions",
  "author": "Gary Bourgeat <gary.bourgeat@gmail.com>",
  "private": false,
  "license": "MIT",
  "keywords": [
    "nestjs",
    "typescript",
    "clean-architecture",
    "domain-driven-design",
    "hexagonal-architecture",
    "ddd",
    "postgresql",
    "typeorm",
    "dijkstra-algorithm",
    "route-planning",
    "rest-api",
    "swagger",
    "openapi",
    "dependency-injection",
    "repository-pattern",
    "value-objects",
    "docker",
    "testing",
    "solid-principles",
    "backend"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gbourgeat/nestjs-ddd-clean-architecture-example.git"
  },
  "bugs": {
    "url": "https://github.com/gbourgeat/nestjs-ddd-clean-architecture-example/issues"
  },
  "homepage": "https://github.com/gbourgeat/nestjs-ddd-clean-architecture-example#readme",
  "scripts": {
    "build": "nest build",
    "format": "biome format --write src test",
    "format:check": "biome format src test",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/presentation/rest-api/rest-api.bootsrap.js",
    "lint": "biome lint --write src test",
    "lint:check": "biome lint src test",
    "check": "biome check --write src test",
    "check:ci": "biome check src test",
    "deps:check": "depcruise --config .dependency-cruiser.js src",
    "deps:graph": "depcruise --config .dependency-cruiser.js --output-type dot src | dot -T svg > docs/dependency-graph.svg",
    "deps:archi": "depcruise --config .dependency-cruiser.js --output-type archi src",
    "test": "npm run test:features",
    "test:watch": "npm run test:features:watch",
    "test:cov": "npm run test:features:cov",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:features": "jest --config test/features/jest.features.json",
    "test:features:watch": "jest --config test/features/jest.features.json --watch",
    "test:features:cov": "jest --config test/features/jest.features.json --coverage",
    "test:e2e": "jest --config test/e2e/jest-e2e.json",
    "test:e2e:cov": "jest --config test/e2e/jest-e2e.json --coverage",
    "test:integration": "jest --config test/integration/jest.integration.json",
    "test:integration:watch": "jest --config test/integration/jest.integration.json --watch",
    "test:integration:cov": "jest --config test/integration/jest.integration.json --coverage",
    "test:all": "npm run test:features:cov && npm run test:integration:cov && npm run test:e2e:cov",
    "test:all:report": "node scripts/merge-coverage.js",
    "docker:dev:up": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:dev:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:e2e:up": "docker-compose -f docker-compose.e2e.yml up -d",
    "docker:e2e:down": "docker-compose -f docker-compose.e2e.yml down",
    "docker:e2e:restart": "docker-compose -f docker-compose.e2e.yml down && docker-compose -f docker-compose.e2e.yml up -d",
    "docker:integration:up": "docker-compose -f docker-compose.integration.yml up -d",
    "docker:integration:down": "docker-compose -f docker-compose.integration.yml down",
    "docker:integration:restart": "docker-compose -f docker-compose.integration.yml down && docker-compose -f docker-compose.integration.yml up -d",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:generate": "npm run typeorm -- migration:generate -d src/infrastructure/database/data-source.ts",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- migration:run -d src/infrastructure/database/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/infrastructure/database/data-source.ts",
    "migration:show": "npm run typeorm -- migration:show -d src/infrastructure/database/data-source.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.ts": [
      "biome check --write --no-errors-on-unmatched"
    ],
    "src/**/*.ts": [
      "bash -c 'npm run deps:check'"
    ]
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/cache-manager": "^3.1.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.5",
    "@nestjs/typeorm": "^11.0.0",
    "axios": "^1.13.2",
    "cache-manager": "^7.2.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^17.2.3",
    "pg": "^8.17.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.28"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "dependency-cruiser": "^17.3.7",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "lint-staged": "^16.2.7",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/test/features/",
      "/test/e2e/"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@test/(.*)$": "<rootDir>/test/$1"
    }
  }
}
